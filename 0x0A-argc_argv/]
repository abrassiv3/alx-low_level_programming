#include <stdio.h>
#include <stdlib.h>
/**
  * main-function name
  * @argc: function parameter
  * @argv: function parameter
  * Return: 0
  */
int main(int argc, char *argv[])
{
	int s;

	if (argc <= 1 && argc > 2)
	{
		printf("Error\n");
	}
	else
	{
		s = atoi(argv[2]);
		printf("%d\n", s);
	}
	return (0);
}
